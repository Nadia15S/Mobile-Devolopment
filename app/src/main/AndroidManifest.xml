<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tugas3"
        tools:targetApi="31">
        <activity
            android:name="com.example.tugas3.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Tugas3"
            tools:ignore="RedundantLabel">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tugas3.UserViewModel"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.tugas3.UserRepository"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.tugas3.User"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.tugas3.UserAdapter"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name="com.example.tugas3.MyApp"
            android:exported="false"
            tools:ignore="DuplicateActivity,Instantiatable" />
    </application>

</manifest>